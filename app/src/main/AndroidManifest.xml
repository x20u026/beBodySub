<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BeBodySub"
        tools:targetApi="34">
        <activity
            android:name=".front.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".front.Diary"
            android:parentActivityName=".front.MainActivity"/>

        <activity
            android:name=".front.DiaryMake" />

        <activity
            android:name=".front.DiaryDisplay" />

        <activity
            android:name=".front.Goal" />

        <activity
            android:name=".front.Plan"
            android:parentActivityName=".front.MainActivity"/>

        <activity
            android:name=".front.PlanDisplay" />

        <activity
            android:name=".front.PlanMake" />

        <activity
            android:name=".front.Syokaikakunin" />

        <activity
            android:name=".front.Syokaitouroku" />


    </application>

</manifest>